    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "^5.2.2"
