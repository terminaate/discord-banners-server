    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.17.16",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.0",
    "@types/react": "^19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-preact": "^1.5.0",
