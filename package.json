  "description": "Discord banners",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start:prod": "cross-env NODE_ENV=prod node dist/index.js",
    "start:dev": "cross-env NODE_ENV=dev tsx watch ./src",
    "format": "prettier --write \"src/**/*\"",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
