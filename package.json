{
  "name": "discord-banners",
  "version": "1.0.0",
  "description": "Discord banners",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=prod node -r module-alias/register ./dist/index.js",
    "start:dev": "tsc-watch --project . --onSuccess \"cross-env NODE_ENV=dev node -r module-alias/register ./dist/index.js\"",
    "format": "prettier --write \"src/**/*\"",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "build": "npm run build:clean && tsc && tsc-alias -p tsconfig.json && npm run build:assets",
    "build:clean": "rimraf dist",
    "build:assets": "tscp",
    "new:module": "hygen new module"
  },
  "repository": "https://github.com/terminaate/game-store-server",
  "keywords": [],
  "author": "Terminaate",
  "license": "MIT",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "module-alias": "^2.2.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2",
    "typescript-cp": "^0.1.9"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "sequelize": "^6.33.0",
    "sqlite3": "^5.1.6"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
