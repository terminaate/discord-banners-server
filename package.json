    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "ioredis": "^5.4.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "preact": "^10.26.4",
